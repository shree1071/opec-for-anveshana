# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to git

# Flask Configuration
SECRET_KEY=your-secret-key-here-change-this
FLASK_ENV=development

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key-here

# Gemini API Keys (Add multiple for automatic rotation)
GEMINI_API_KEY_1=your-gemini-api-key-1
GEMINI_API_KEY_2=your-gemini-api-key-2
GEMINI_API_KEY_3=your-gemini-api-key-3
# Add more keys as GEMINI_API_KEY_4, GEMINI_API_KEY_5, etc.

# Redis Configuration (Optional - for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60

# Clerk Authentication (if using)
CLERK_SECRET_KEY=your-clerk-secret-key
# Voice Agent Configuration
VITE_ELEVENLABS_AGENT_ID=your-career-coach-agent-id
VITE_ELEVENLABS_INTERVIEW_AGENT_ID=your-interviewer-agent-id

# Tavus 3D Avatar Configuration
TAVUS_API_KEY=your-tavus-api-key
TAVUS_REPLICA_ID=your-replica-id
TAVUS_PERSONA_ID=your-persona-id
